{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/template/index.html"
    }
  ],
  "cleanUrls": true,
  "env": {
    "OPENAI_API_KEY": "$OPENAI_API_KEY",
    "SERPAPI_API_KEY": "$SERPAPI_API_KEY",
    "ZAPIER_NLA_API_KEY": "$ZAPIER_NLA_API_KEY"
  }
}
